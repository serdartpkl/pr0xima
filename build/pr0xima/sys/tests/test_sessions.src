Proxima.testing.tests.test_sessions=function
TestSuite_Sessions=function
Test_GetActiveSessions=function
getActiveSessions=Proxima.sessions.getSessions
expect(getActiveSessions).notToBeNull
end function
Test_GetSavedSessions=function
getSavedSessions=Proxima.sessions.getSessions(true)
expect(getSavedSessions).notToBe([])
end function
Test_CreateLocalSession=function
localIP=get_shell.host_computer.local_ip
createSession=Proxima.sessions.createSession(0,localIP,get_router(localIP))
expect(createSession).notToBeNull
end function
Test_CreateRemoteSession=function
remoteIP="128.34.2.99"
createSession=Proxima.sessions.createSession(1,remoteIP,get_router(remoteIP))
expect(createSession).notToBeNull
end function
Test_SetActiveSession=function
setSession=Proxima.sessions.setSession(1)
expect(setSession).toBeTruthy
end function
Test_GetSessionValue=function
getSessionValue=Proxima.sessions.getSessionValue(Proxima.sessions.activeSession,"payloadInjection")
expect(getSessionValue).toBe(1)
end function
Test_SetSessionValue=function
setSessionValue=Proxima.sessions.setSessionValue(Proxima.sessions.activeSession,"payloadInjection",0)
expect(setSessionValue).toBeTruthy
end function
Test_isActiveSessionLocal=function
isSessionLocal=Proxima.sessions.isSessionLocal(Proxima.sessions.activeSession)
expect(isSessionLocal).toBeTruthy
end function
Test_isActiveSessionRemote=function
isSessionRemote=Proxima.sessions.isSessionRemote(Proxima.sessions.activeSession)
expect(isSessionRemote).toBeTruthy
end function
it("GET_ACTIVE_SESSIONS",@Test_GetActiveSessions)
it("GET_SAVED_SESSIONS",@Test_GetSavedSessions)
it("CREATE_LOCAL_SESSION",@Test_CreateLocalSession)
it("CREATE_REMOTE_SESSION",@Test_CreateRemoteSession)
it("GET_ACTIVE_SESSIONS",@Test_GetActiveSessions)
it("SET_ACTIVE_SESSION",@Test_SetActiveSession)
it("GET_SESSION_VALUE",@Test_GetSessionValue)
it("SET_SESSION_VALUE",@Test_SetSessionValue)
it("GET_SESSION_VALUE",@Test_GetSessionValue)
it("IS_ACTIVE_SESSION_LOCAL",@Test_isActiveSessionLocal)
it("IS_ACTIVE_SESSION_REMOTE",@Test_isActiveSessionRemote)
end function
describe("SESSIONS_TEST_SUITE",@TestSuite_Sessions)
end function